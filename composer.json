{
  "type": "project",
  "license": "proprietary",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": ">=8.1",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "ext-mongodb": "*",
    "ext-redis": "*",
    "beberlei/assert": "^3.3.2",
    "doctrine/annotations": "^1.14.3",
    "doctrine/mongodb-odm-bundle": "^4.5.2",
    "gesdinet/jwt-refresh-token-bundle": "^1.1.1",
    "lcobucci/clock": "^3.0",
    "lexik/jwt-authentication-bundle": "^2.18.1",
    "piscibus/php-hashtag": "^1.1",
    "sensio/framework-extra-bundle": "^6.2.10",
    "symfony/console": "^6.2.7",
    "symfony/dotenv": "^6.2.7",
    "symfony/flex": "^2.2.5",
    "symfony/form": "^6.2.5",
    "symfony/framework-bundle": "^6.2.5",
    "symfony/property-access": "^6.2.5",
    "symfony/property-info": "^6.2.7",
    "symfony/runtime": "^6.2.5",
    "symfony/security-bundle": "^6.2.8",
    "symfony/serializer": "^6.2.5",
    "symfony/validator": "^6.2.7",
    "symfony/yaml": "^6.2.7",
    "voku/portable-ascii": "^2.0.1"
  },
  "require-dev": {
    "fakerphp/faker": "^1.21",
    "friendsofphp/php-cs-fixer": "^3.14.2",
    "phpunit/phpunit": "^9.6.4",
    "psalm/plugin-phpunit": "^0.18.4",
    "psalm/plugin-symfony": "^4.0.2",
    "roave/security-advisories": "dev-latest",
    "symfony/browser-kit": "^6.2",
    "symfony/css-selector": "^6.2",
    "symfony/maker-bundle": "^1.48",
    "symfony/phpunit-bridge": "^6.2.7",
    "symfony/var-dumper": "^6.2",
    "vimeo/psalm": "^4.30"
  },
  "config": {
    "platform": {
      "php": "8.1",
      "ext-mongodb": "1.14.0"
    },
    "allow-plugins": {
      "composer/package-versions-deprecated": true,
      "symfony/flex": true,
      "symfony/runtime": true
    },
    "optimize-autoloader": true,
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php73": "*",
    "symfony/polyfill-php74": "*",
    "symfony/polyfill-php80": "*",
    "symfony/polyfill-php81": "*"
  },
  "scripts": {
    "check-style": "vendor/bin/php-cs-fixer fix --dry-run --allow-risky=yes --format=txt --verbose",
    "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes",
    "psalm": "vendor/bin/psalm --show-info=true --no-cache --shepherd --stats",
    "test": "php bin/phpunit --testdox",
    "console": "php bin/console",
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": "true",
      "require": "6.1.*"
    }
  }
}
